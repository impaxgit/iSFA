<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="45dp"
        android:text="@string/uom" />

    <TextView
        android:id="@+id/deliverOrderLines_txtDescription"
        android:layout_width="294dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="10dp"
        android:layout_toEndOf="@+id/textView10"
        android:fontFamily="serif-monospace"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:typeface="monospace" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/deliverOrderLines_txtDescription"
        android:layout_alignParentStart="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="14dp"
        android:text="@string/Qty" />

    <TextView
        android:id="@+id/deliverOrderLines_txtQty"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/deliverOrderLines_txtDescription"
        android:layout_alignStart="@+id/deliverOrderLines_txtDescription"
        android:layout_marginStart="2dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="serif-monospace"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="monospace" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="124dp"
        android:text="@string/Amount" />

    <TextView
        android:id="@+id/deliverOrderLines_txtAmount"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/deliverOrderLines_txtDescription"
        android:layout_alignParentTop="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="112dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="serif-monospace"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:typeface="monospace" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="163dp"
        android:text="@string/OrderDate" />

    <TextView
        android:id="@+id/deliverOrderLines_txtOrderDate"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/deliverOrderLines_txtAmount"
        android:layout_alignStart="@+id/deliverOrderLines_txtDescription"
        android:layout_marginStart="1dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="serif-monospace"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:typeface="monospace" />

    <CheckBox
        android:id="@+id/deliverOrderLines_chkdeliver"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="7dp"
        android:text="@string/deliverAll" />

</RelativeLayout>